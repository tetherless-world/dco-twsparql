PREFIX rdfs:  <http://www.w3.org/2000/01/rdf-schema#>
PREFIX dco: <http://info.deepcarbon.net/schema#>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>

#This query gets the names of each DCO community and counts the number of users in 
#each community
SELECT ?dcocommname (COUNT(?user) as ?number) WHERE
{
?user a foaf:Person;
         <http://vivo.mydomain.edu/ns#networkId> ?netID;
         dco:associatedDCOCommunity ?dcocomm.
?dcocomm rdfs:label ?dcocommname.
}
GROUP BY ?dcocommname
